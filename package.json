{
  "name": "iwf-typescript-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "module": "main.js",
  "types": "dist/index.ts",
  "scripts": {
    "build:all": "npm run gen && npm run build",
    "build": "tsc",
    "test": "node --test",
    "retest": "yarn build && yarn test",
    "gen": "openapi-generator-cli generate && yarn typed-openapi iwf-idl/iwf-sdk.yaml -o gen/api-schema.ts --runtime zod",
    "unnecessary": "openapi-typescript iwf-idl/iwf-sdk.yaml -o gen/iwf-sdk.ts ",
    "clean": "rm -rf ./gen && rm -rf ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "esm": "^3.2.25",
    "fastify": "^5.2.2",
    "fastify-type-provider-zod": "^4.0.2",
    "iwfidl": "file:gen/iwfidl",
    "openapi-typescript": "^7.6.1",
    "tsx": "^4.19.3",
    "typed-openapi": "^0.10.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.9.1"
