{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"rewriteRelativeImportExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"allowJs": false,
		"lib": ["ESNext", "dom"],
		"module": "NodeNext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"outDir": "./dist"
	},
	"include": [
		"iwf",
		"test"
	],
	"exclude": [
		"dist",
		"node_modules"
	]
}
